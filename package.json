{
  "name": "@innv/ts-typeforge",
  "version": "0.1.0",
  "description": "Criação automática de mocks de DTOs e tipos complexos em TypeScript usando ts-morph e Faker.",
  "author": "Innovare",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innovare-tech/ts-typeforge.git"
  },
  "bugs": {
    "url": "https://github.com/innovare-tech/ts-typeforge/issues"
  },
  "homepage": "https://github.com/innovare-tech/ts-typeforge/issues#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "build:clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.build-cjs.json",
    "build:esm": "tsc -p tsconfig.build-esm.json",
    "build:types": "tsc -p tsconfig.build-types.json",
    "build": "npm run build:clean && npm run build:cjs && npm run build:esm && npm run build:types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "prepare": "husky install",
    "prepublishOnly": "npm run lint && npm test && npm run build"
  },
  "peerDependencies": {
    "@faker-js/faker": "^8.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "ts-morph": "^19.0.0 || ^20.0.0 || ^21.0.0 || ^22.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.38.0",
    "@faker-js/faker": "^8.4.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.4.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.2",
    "ts-morph": "^22.0.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.46.2"
  },
  "keywords": [
    "typescript",
    "mock",
    "fake",
    "dto",
    "testing",
    "faker",
    "ts-morph",
    "generator",
    "factory"
  ]
}
